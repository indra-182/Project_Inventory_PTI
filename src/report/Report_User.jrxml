	<parameter name="user" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
    a.id_karyawan     AS mst_user_id_karyawan,
    b.nama_lengkap    AS mst_karyawan_nama_lengkap,
    a.username        AS mst_user_username,
    a.last_login      AS mst_user_last_login,
    c.group_name      AS mst_group_group_name
FROM mst_user a
LEFT JOIN mst_karyawan b 
    ON a.id_karyawan = b.id_karyawan
LEFT JOIN mst_group c 
    ON a.id_group = c.id_group
WHERE a.sts_active = 1
ORDER BY a.last_login ASC;]]>
	</queryString>
